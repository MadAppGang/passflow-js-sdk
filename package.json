{
  "name": "@passflow/passflow-js-sdk",
  "packageManager": "yarn@4.2.2",
  "description": "Passflow JS SDK",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn build:js --watch & yarn build:dts --watch",
    "build:dts": "tsc --build tsconfig.json",
    "build:js": "node ./esbuild.config.cjs",
    "build": "yarn build:dts && yarn build:js",
    "test": "jest",
    "lint": "eslint lib --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@eslint/js": "^9.1.1",
    "@types/jest": "^29.5.12",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.20.2",
    "eslint": "8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.2",
    "typescript": "5.2.2",
    "typescript-eslint": "^7.8.0"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/types": "^10.0.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "dayjs": "^1.11.11",
    "uuid": "^9.0.1"
  },
  "authors": [
    {
      "name": "Ivan Holiak",
      "url": "https://github.com/IvanHoliak"
    },
    {
      "name": "Nikita Kysil",
      "url": "https://github.com/mag-nikita-ks"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/passflow-js-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/passflow-js-sdk.git",
    "directory": "packages/passflow-js-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "passflow",
    "auth",
    "authentication",
    "passwordless",
    "jwt",
    "passkey"
  ]
}
