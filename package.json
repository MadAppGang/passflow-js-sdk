{
  "name": "@passflow/passflow-js-sdk",
  "description": "Passflow JS SDK",
  "version": "0.1.43",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "dev": "pnpm vite",
    "watch": "vite build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build:dts": "tsc --build tsconfig.json",
    "build": "pnpm clean && pnpm vite build && pnpm build:dts",
    "test": "vitest run --reporter=default",
    "test:services": "vitest run tests/services",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "format": "biome format . --write",
    "lint:format:fix": "biome check . --apply && biome format . --write",
    "ci": "biome ci ."
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@biomejs/biome": "1.9.4",
    "@types/sinon": "^17.0.4",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "sinon": "^19.0.5",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/types": "^12.0.0",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "uuid": "^11.1.0"
  },
  "authors": [
    {
      "name": "Ivan Holiak",
      "url": "https://github.com/IvanHoliak"
    },
    {
      "name": "Nikita Kysil",
      "url": "https://github.com/mag-nikita-ks"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/passflow-js-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/passflow-js-sdk.git"
  },
  "pnpm": {
    "overrides": {}
  },
  "packageManager": "pnpm@10.8.0"
}
