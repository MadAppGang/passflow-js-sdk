{
  "name": "@passflow/passflow-js-sdk",
  "description": "Passflow JS SDK",
  "version": "0.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm vite",
    "build:dts": "tsc --build tsconfig.json",
    "build:js": "node ./esbuild.config.cjs",
    "build": "pnpm vite build",
    "test": "jest",
    "lint": "biome check .",
    "format": "biome format . --write",
    "ci": "biome ci ."
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@biomejs/biome": "1.9.4",
    "@types/jest": "^29.5.12",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.24.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sinon": "^19.0.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/types": "^12.0.0",
    "axios": "^1.6.8",
    "axios-retry": "^4.1.0",
    "dayjs": "^1.11.11",
    "uuid": "^11.0.5"
  },
  "authors": [
    {
      "name": "Ivan Holiak",
      "url": "https://github.com/IvanHoliak"
    },
    {
      "name": "Nikita Kysil",
      "url": "https://github.com/mag-nikita-ks"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/passflow-js-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/passflow-js-sdk.git"
  }
}
