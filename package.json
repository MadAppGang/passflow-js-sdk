{
  "name": "@aooth/aooth-sdk-js",
  "packageManager": "yarn@4.1.1",
  "description": "Aooth SDK JS",
  "version": "0.0.19",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/lib/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "yarn build:js --watch & yarn build:dts --watch",
    "build:dts": "tsc --build tsconfig.json",
    "build:js": "node ./esbuild.config.cjs",
    "build": "yarn build:dts && yarn build:js",
    "test": "jest",
    "lint": "eslint lib --ext ts --report-unused-disable-directives --max-warnings 0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.19.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@simplewebauthn/browser": "^9.0.0",
    "@simplewebauthn/types": "^9.0.1",
    "axios": "^1.5.1",
    "axios-retry": "^4.1.0",
    "dayjs": "^1.11.10",
    "uuid": "^9.0.1"
  },
  "authors": [
    {
      "name": "Ivan Holiak",
      "url": "https://github.com/IvanHoliak"
    },
    {
      "name": "Nikita Kysil",
      "url": "https://github.com/mag-nikita-ks"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MadAppGang/aooth-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MadAppGang/aooth-js.git",
    "directory": "packages/aooth-sdk-js"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "aooth",
    "auth",
    "authentication",
    "passwordless",
    "jwt",
    "passkey"
  ]
}
